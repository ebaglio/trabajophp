     <?php

      class Usuario{
      private $nombre;
      private $rol;
      private $ip;
      private $fecha;

      public function getEdad()
      {return $this->edad;}
      public function setEdad($edad)
      {$this->edad = $edad;}

      public function getNombre()
      {return $this->nombre;}

      public function setClave($clave)
      {$this->clave = $clave;}
      }
      ?>
