     <?php

      class Usuario{
      private $nombre;
      private $rol;
      private $ip;
      private $fecha;

	  function __construct($nombre,$rol)
      {            
             $this->nombre = $nombre;
             $this->rol = $rol;
	  }
	  
	  function __construct($nombre)
      {            
             $this->nombre = $nombre;
             $this->rol = "pisapapeles";
	  }		
		
      public function getNombre()
      {return $this->nombre;}
      public function getNombre($nombre)
      {$this->nombre = $nombre;}

      public function getIp()
      {return $this->ip;}
      public function setIp($ip)
      {$this->ip = $ip;}           

      public function getFecha()
      {return $this->fecha;}
      public function getFecha($nombre)
      {$this->fecha = $fecha;}
     
      }
      ?>
